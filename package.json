{
  "name": "clankerrank",
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
	"lint:fix": "turbo run lint:fix",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "check-types": "turbo run check-types",
	"db:generate": "cd packages/db && turbo run db:generate",
	"db:migrate": "cd packages/db && turbo run db:migrate",
	"db:push": "cd packages/db && turbo run db:push",
	"db:studio": "cd packages/db && turbo run db:studio"
  },
  "devDependencies": {
    "prettier": "^3.6.2",
    "turbo": "^2.6.1",
    "typescript": "5.9.2"
  },
  "engines": {
    "node": ">=18"
  },
  "packageManager": "bun@1.1.10",
  "workspaces": [
    "apps/*",
    "packages/*"
  ]
}
